{
    "buildCommand": "npm install",
    "outputDirectory": "./",
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/.netlify/functions/send-quote",
            "destination": "/api/send-quote"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}